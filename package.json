{
  "name": "tech-db",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start_fail": "pm2 start processes.json --no-daemon",
    "start:dev": "nodemon --exec babel-node app.js",
    "server": "NODE_ENV=production babel-node app.js",
    "start": "webpack --config base.webpack.config.js app.js && NODE_ENV=production pm2-runtime start -i max public/bundle.js",
    "start1": "NODE_ENV=production pm2-runtime start -i max babel-node -- app.js"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-optional-chaining"
    ]
  },
  "dependencies": {
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.3",
    "pg": "^8.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
